			<div id="guest-live-chat">
				<a href="../javascript:;" title="Live Chat">Live Chat</a>
			</div><!-- #guest-live-chat -->

			<div class="clear"></div>

			<div id="menu">
				<ul>
					<li class="menu-left-corner"></li>
					<li><a href="../index.html" title="">HOME</a></li>
					<li><a href="../about-us.html" title="">ABOUT US</a></li>
