					<li class="menu-login">
						<a href="../javascript:;" title="Login" class="menu-login-link">Login</a>

						<ul id="loginbox">
							<form method="post" action="vars.php" id="loginform" name="loginform">
								<li class="login-username">
									<input type="text" tabindex="10" class="input" id="user_login" name="log" value="Username" onblur="if (this.value == '') {this.value = 'Username';}"  onfocus="if (this.value == 'Username') {this.value = '';}">
								</li>

								<li class="login-password">
									<input type="password" tabindex="20" class="input" id="user_pass" name="pwd" value="Password" onblur="if (this.value == '') {this.value = 'Password';}"  onfocus="if (this.value == 'Password') {this.value = '';}">
								</li>

								<div class="clear"></div>

								<li class="login-forgot">
									<a href="../javascript:;" title="Forgot Password">Forgot Password</a>
								</li>

								<li class="login-submit">
									<input type="submit" tabindex="100" value="OK" class="button-primary" id="wp-submit" name="wp-submit">
								</li>

								<div class="clear"></div>
							</form>
						</ul><!-- #loginbox -->
					</li>
				</ul>
			</div><!-- #menu -->

			<div class="clear"></div>
		</div><!-- .fixed -->
	</div><!-- #header -->

